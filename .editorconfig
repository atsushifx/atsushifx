# EditorConfig is awesome: https://EditorConfig.org
#
# default editorconfig for github
#

# top-most EditorConfig file
#
# root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# default character-set
charset = utf-8

###

## editorconfig
#
[**.editorconfig]
indent_style = tab
indent_size = tab
tab_width = 2

## markdown
#
[**.md]
indent_style = space
indent_size = tab
tab_width = 2

